@model List<Payroll_Application.Models.BranchEntity>
<!-- Modal -->
<div class="modal fade" id="popupStaffList" tabindex="-1" role="dialog" aria-labelledby="popupStaffLabel" aria-hidden="true" data-backdrop="static">
    <div class="modal-dialog  modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header" style="border:2px solid silver; background-color:green;color:white;">
                <h5 class="modal-title" id="exampleModalLabel">STAFF LIST</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" style="color:white;opacity:1;">
                    <span aria-hidden="true" >&times;</span>
                </button>
            </div>
            <div class="modal-body" id="listofEmployee">
                
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
